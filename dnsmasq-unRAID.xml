<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Beta>False</Beta>
    <Category>Network:DNS</Category>
    <Changes>
[font size=3]2019.11.03[/font]
- Initial version.
    </Changes>



    <Name>mydnsmasq</Name>
    <Description>
        Trying out dnsmasq in Docker.
    </Description>
    <Registry>https://registry.hub.docker.com/u/codesaru/dnsmasq</Registry>
    <Repository>codesaru/dnsmasq</Repository>
    <WebUI/>
    <Icon/>

    <Environment>
        <Variable>
            <Name>EXTRA_PARAMS</Name>
            <Value>--address=8.8.8.8 --address=8.8.4.4 --server=/example.com/192.168.0.42</Value>
        </Variable>
    </Environment>

    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <HostPort>53</HostPort>
            <ContainerPort>53</ContainerPort>
            <Protocol>tcp</Protocol>
        </Publish>
        <Publish>
            <HostPort>53</HostPort>
            <ContainerPort>53</ContainerPort>
            <Protocol>udp</Protocol>
        </Publish>
    </Networking>

    <Data/>

    <ExtraParams></ExtraParams>
</Container>

